{
  "name": "@typescript-entity/core",
  "version": "1.0.0",
  "description": "Typed entity library with attribute sanitization and validation.",
  "keywords": [
    "typescript",
    "entity",
    "sanitize",
    "sanitization",
    "validate",
    "validation"
  ],
  "homepage": "https://github.com/apancutt/typescript-entity#readme",
  "bugs": {
    "url": "https://github.com/apancutt/typescript-entity/issues"
  },
  "author": "Adam Pancutt <adam@pancutt.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apancutt/typescript-entity.git"
  },
  "files": [
    "build/**/*"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "lodash": "4",
    "validator": "13"
  },
  "devDependencies": {
    "@types/jest": "26",
    "@types/lodash": "4",
    "@types/node": "14",
    "@types/validator": "13",
    "jest": "26",
    "rimraf": "3",
    "ts-jest": "26",
    "typescript": "4"
  },
  "scripts": {
    "prebuild": "rimraf ./build/",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "yarn run build --watch",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx src __tests__"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx|js)$"
  }
}
