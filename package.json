{
  "name": "typescript-model",
  "version": "1.2.2",
  "description": "Typed entity modeling library with attribute sanitization and validation.",
  "keywords": [
    "typescript",
    "entity",
    "model",
    "sanitize",
    "sanitization",
    "validate",
    "validation"
  ],
  "homepage": "https://github.com/apancutt/typescript-model#readme",
  "bugs": {
    "url": "https://github.com/apancutt/typescript-model/issues"
  },
  "author": "Adam Pancutt <adam@pancutt.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apancutt/typescript-model.git"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@types/validator": "10",
    "validator": "11"
  },
  "devDependencies": {
    "@types/jest": "24",
    "@types/node": "12",
    "jest": "24",
    "ts-jest": "24",
    "tslint": "5",
    "tslint-config-airbnb": "5",
    "tsutils": "3",
    "typescript": "3"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && tsc -p tsconfig.build.json",
    "build:watch": "npm run build -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "lint": "tslint --project tsconfig.build.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*\\.test\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
